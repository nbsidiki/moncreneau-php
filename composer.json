    "autoload-dev": {
        "psr-4": {
            "Moncreneau\\Tests\\": "tests/"
        }
    },
    "keywords": [
        "moncreneau",
        "api",
        "appointments",
        "booking"
    ],
    "homepage": "https://github.com/moncreneau/moncreneau-php"
}